def bucle_10():
    #SUMA DE NUMEROS PARES E IMPARES
    nfinal= input("hasta qué numero quieres sumar: ")
    for numero in range(1,nfinal+1):
        #para cada numero e pregunto si es par o impar
        if(numero%2==0):
            print str(numero),"es par lmao"
        else:
            print str(numero), "es impar xd"

bucle_10()
